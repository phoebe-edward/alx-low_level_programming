/**
 * op_add - function
 * @a: first parameter
 * @b: second parameter
 * Return: int result
 */
int op_add(int a, int b)
{
	return (a + b);
}
/**
 * op_add - function
 * @a: first parameter
 * @b: second parameter
 * Return: int result
 */
int op_sub(int a, int b)
{
        return (a - b);
}
/**
 *op_mul - mul function
 * @a: first param
 * @b: second param
 * Return: int result
 */
int op_mul(int a, int b)
{
	return (a * b);
}
/**
 * op_div - div function
 * @a: first param
 * @b: second param
 * Return: div result
 */
int op_div(int a, int b)
{
	return (a / b);
}
/**
 * op_mod - modulu function
 * @a: first param
 * @b: second param
 * Return: remainder
 */
int op_mod(int a, int b)
{
	return (a % b);
}
