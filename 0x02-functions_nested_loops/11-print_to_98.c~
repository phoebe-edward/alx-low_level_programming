#include <stdio.h>

/**
 * print_to_98 - prints integers from n to 98
 * @n: given starting integer
 */

void print_to_98(int n)
{
	int i;

	if (n <= 98)
	{
		for (i = n; i < 99; i++)
		{
			if (i != n)
				printf(", ");
			printf("%d", i);
		}
	}
	else
	{
		for (i = n; n >= 98; i--)
		{
			if (i != n)
				printf(", ");
			printf("%d", i);
		}
	}
	printf("\n");
}
